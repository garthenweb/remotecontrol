{
  "extends": "airbnb",
  "env": {
    "jasmine": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": 0,
    "object-curly-newline": 0,
    "arrow-parens": 0,
    "function-paren-newline": 0
  }
}
